{{#each application}}
<div class="row">
    <div class="col-12 col-md-4">
        <div class="card card-body pull-left mb-2">
            <h4>{{name}}</h4>
            {{#if status}}
            <h5>Status {{status}}</h5>
            {{else}}
            <h5> Status Pending</h5>
            {{/if}}
        </div>
    </div>
</div>


{{/each}}